	function saveClinicAd(idx,lay){
		    var saImgPath = $("#saImgPath",lay).val();
			if(saImgPath == ""){
				top.layer.msg("请先上传图片！");
				return;
			}
			
			var saId = $("#saId",lay).val();
			var exeUrl = "";
			if(saId == ""){
				exeUrl = "/api/clinicAd/add";
			}else{
				exeUrl = "/api/clinicAd/update";
			}
			
			var saAppPic = $("#saAppPic",lay).val();
			var saLinkUrl = $("#saLinkUrl",lay).val();
			var saStartTime = $("#saStartTime",lay).val();
			var saEndTime = $("#saEndTime",lay).val();
			var adPgCode = $("#adPgCode",lay).val();
			if(adPgCode != 0 && saAppPic == ""){
				top.layer.msg("请上传App图片！");
				return ;
			}
			
			var adPageParam = "";
			var param = {};
			if(adPgCode == 1001){
				param.url = $("#url",lay).val();
				if(param.url == ""){
					top.layer.msg("请输入H5链接！");
				    return ;
				}
				param.title = $("#title",lay).val();
				param.hasTitle = $("#hasTitle",lay).val();
			}else if(adPgCode == 1054){
				param.s_key = $("#s_key",lay).val();
				if(param.s_key == ""){
					top.layer.msg("请输入关键词！");
				    return ;
				}
				param.s_supplier = $("#s_supplier",lay).val();
				param.sellerId = $("#sellerId",lay).val();
				param.suDomainPrefix = $("#suDomainPrefix",lay).val();
			}else if(adPgCode == 1005){
				param.gId = $("#gId",lay).val();
				if(param.gId == ""){
					top.layer.msg("请输入商品ID！");
				    return ;
				}
				param.siteId = $("#siteId",lay).val();
			}else if(adPgCode == 1013){
				param.suDomainPrefix = $("#suDomainPrefix",lay).val();
				param.suType = $("#suType",lay).val();
			}
			
			if(adPgCode != 0){
				adPageParam = $.toJSON(param);
			}
			
			var ad = {};
			ad.saId = saId;
			ad.saImgPath = saImgPath;
			ad.saAppPic = saAppPic;
			ad.saLinkUrl = saLinkUrl;
			ad.saStartTime = saStartTime;
			ad.saEndTime = saEndTime;
			ad.adPgCode = adPgCode;
			ad.adPageParam = adPageParam;
			top.ue.load();
			$.ajax({
	            url: exeUrl,
	            type: 'POST',
	            data: {"ad":$.toJSON(ad)},
	            dataType: 'json',
	            complete:function(){
	         	   top.ue.closeAll("loading");
	            },
	            success: function(result) {
	                if (result.code == 0) {
	                   top.ue.close(idx);
	                   //location.href = "/api/clinicZone/main";
                        getPageData();
	                }else{
	                	top.layer.msg("系统出现异常！");	
	                }
	            },
	            error: function(){
	         	   top.layer.msg("系统出现异常！");
	            }
	        });
	}


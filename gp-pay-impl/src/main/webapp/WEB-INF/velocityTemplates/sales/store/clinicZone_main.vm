	function loadAppPages(lay,defValue){
		
		$("#adPgCode,#clPgCode",lay).select2({
		        placeholder: $(this).attr("placeholder"),
		        tags: "true",
		        allowClear: false,
		        minimumResultsForSearch: -1,
		        language: "zh-CN",
		        width: 200,
		        cache: true,
		        ajax1: {
		            url: '/api/clinicZone/appPages',
		            processResults: function (res) {
		                var data = {
		                    results: []
		                }
		                data.results.push({
		                    id: "0",
		                    text: "请选择"
		                });
		                $.each(res.data, function (i, e) {
		                    var _arr = {
		                        id: e.pageCode,
		                        text: e.pageName
		                    };
		                    if(defValue && defValue == e.pageCode){
		                    	_arr.selected = true;
		                    }
		                    data.results.push(_arr);
		                })
		                return data;
		            }
		        }
		    }).unbind('select2:select').on('select2:select', function (e) {
		    	var value = $("#adPgCode,#clPgCode",lay).val();
				$(".clinic-app",lay).each(function(i,o){
					if(value == 0){
						$(o).hide();
					}else{
						if($(o).attr("data-value") == value){
							$(o).show();
						}else{
							$(o).hide();
						}
					}
				});
		        
		    });
		
		$.ajax({
			url:'/api/clinicZone/appPages',
			success:function(res){
				$.each(res.data, function (i, e) {
					var opt = "";
					if(defValue == e.pageCode){
						opt = "<option value="+e.pageCode+" selected>"+e.pageName+"</option>"
					}else opt = "<option value="+e.pageCode+" >"+e.pageName+"</option>"
					$("#adPgCode,#clPgCode",lay).append(opt).trigger("select2:select");
				})
			}
		})
	}
	
	
	
	function initFileUpload(lay) {

			param.suDomainPrefix = $("#suDomainPrefix",lay).val();
			param.s_key = $("#s_key",lay).val();
			if(param.s_key == ""){
				top.layer.msg("请输入关键词！");
			    return ;
			}
		}else if(adPgCode == 1005){
			param.gId = $("#gId",lay).val();
			param.siteId = $("#siteId",lay).val();
			if(param.gId == '') {
				top.layer.msg("请填写商品ID！");	
				return;
			}
		}else if(adPgCode == 1013){
			param.suDomainPrefix = $("#suDomainPrefix",lay).val();
			param.suType = $("#suType",lay).val();
		}
		
		var adPageParam = "";
		if(adPgCode>0){
			adPageParam = $.toJSON(param);
		}
		var cf = {
			cfId : cfId,
			cfName : cfName,
			cfDesc : cfDesc,
			cfMoreLink : cfMoreLink,
			adPgCode : adPgCode,
			adPageParam :adPageParam
		};
		top.ue.load();

		$.ajax({
            url: exeUrl,
            type: 'POST',
            data: {"cf":$.toJSON(cf)},
            dataType: 'json',
            complete:function(){
         	   top.ue.closeAll("loading");
            },
            success: function(result) {
                if (result.code == 0) {
                   top.ue.close(idx);
                   //location.href = "/api/clinicZone/main";
                    getPageData()
                }else{
                	top.layer.msg(result.message);
                }
            },
            error: function(){
         	   top.layer.msg("系统出现异常！");
            }
        });
	}


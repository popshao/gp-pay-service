	//上移
	function moveUpAd(id,el,type){
	   var idArray = [];
	   var _cur = {},_prev = {};
	   
	   var prevId = $(el).parents('tr').prev().find('input[name="itemId"]').val();
	   var prevOrder = $(el).parents('tr').prev().find('input[name="itemOrder"]').val();
	   var curOrder = $(el).siblings('input[name="itemOrder"]').val();

	    var url,dataJson;
	    if(type) {  //  楼层
	 		_cur.cfId = id;
	 	    _cur.cfOrder = prevOrder;
	 	    idArray.push(_cur);

	 	    _prev.cfId = prevId;
	 	    _prev.cfOrder = curOrder;
	 	    idArray.push(_prev);

	 		url = '/api/clinicFloor/move';
	 		dataJson = {"cfs":$.toJSON(idArray)}
	    }
	    else {  //  广告
	 		_cur.saId = id;
	 	    _cur.saOrder = prevOrder;
	 	    idArray.push(_cur);

	 	    _prev.saId = prevId;
	 	    _prev.saOrder = curOrder;
	 	    idArray.push(_prev);

	 		url = '/api/clinicAd/move';
	 		dataJson = {"ads":$.toJSON(idArray)}
	    }

	   $.ajax({
           url: url,
           type: 'POST',
           data: dataJson,
           dataType: 'json',
           complete:function(){
        	   top.ue.closeAll("loading");
           },
           success: function(result) {
               if (result.code == 0) {
            	   top.layer.msg("操作成功！");	
            	   //window.location.reload(true);
                   getPageData();
               }else{
               	  top.layer.msg("系统出现异常！");	
               }
           },
           error: function(){
        	   top.layer.msg("系统出现异常！");
           }
       });
	   
	}
	

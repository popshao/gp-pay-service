			exeUrl = "/api/clinicFloor/update";
		}

		if(cfName == ""){
			top.layer.msg("请填写楼层名称！");
			return;
		}

		var param = {};
		if(adPgCode == 1001){
			param.url = $("#url",lay).val();
			param.title = $("#title",lay).val();

  <body>
    <div class="container">
        	#parse("common/header_top.vm")
       #parse("common/header.vm")
      <div class="content">
        <div class="wrap cc">
         #parse("common/left_menu_sales.vm")
          <div class="user_right fl">
            <!-- 用户中心_主体内容_S -->
            <div class="indmain">
                <div class="path">
                	<p><a href="/sales/index.html" class="color">销售中心 </a>&gt; 商品管理 &gt; 商品信息修改</p>
                </div>
                <h3 class="info_title">商品信息</h3>
                <div class="info_sheet cc">
                	<form action="" method="post" id="">
                    <input type="hidden" id="taxrateMin" name="taxrateMin" value="$!{taxrateMin}" />
                    <input type="hidden" id="taxrateMax" name="taxrateMax" value="$!{taxrateMax}" />
                	<input type="hidden" id="gId" name="gId"  value="$!{goods.gId}"/>
                        <ul>
                            <li>
                                <label>通用名（化学名）<font color="red">*</font>：</label>

                                <input class="input" name="gName" type="text" value="$!{goods.gName}" id="gName" onblur="getFisrtSpell(this,'通用名必填.',$('#gName_info'),'gNameCode',this.value,$('#gNameCode_info'));">

                                <label class="help-tips" id="gName_info"></label>
                            </li>
                            <li>
                                <label>商品助记码<font color="red">*</font>：</label>
                                <input class="input" name="gNameCode" type="text" value="$!{goods.gNameCode}" id="gNameCode" onblur="isEmptyContent(this,'商品助记码必填.',$('#gNameCode_info'));">
                                <label class="help-tips" id="gNameCode_info"></label>
                            </li>
                            <li>
                                <label>商品名：</label>
                                <input class="input" name="gCommonName" type="text" value="$!{goods.gCommonName}" id="gCommonName"  >
                            </li>
                            <li>
                                <label>别名：</label>
                                <input class="input" name="gAlias" type="text" value="$!{goods.gAlias}" id="gAlias">
                            </li>
                            <li>
                                <label>商品编码：</label>
                                <input class="input" name="gSn" type="text" value="$!{goods.gSn}" id="gSn" maxlength="60">
                            </li>
                            <li>
                                <label>批准文号<font color="red">*</font>：</label>
                                <input class="input" name="gLicenseNo" type="text" value="$!{goods.gLicenseNo}" id="gLicenseNo" onblur="isEmptyContent(this,'批准文号必填.',$('#gLicenseNo_info'));">
                                <label class="help-tips" id="gLicenseNo_info"></label>
                            </li>
                            <li>
                                <label>生产厂家<font color="red">*</font>：</label>
                                <input class="input" name="gManufacture" type="text" value="$!{goods.gManufacture}" id="gManufacture"  onblur="getFisrtSpell(this,'生产厂家必填.',$('#gManufacture_info'),'gManufactureCode',this.value,$('#gManufactureCode_info'));">
                                <label class="help-tips" id="gManufacture_info"></label>
                            </li>
                            <li>
                                <label>生产厂家助记码：</label>
                                <input class="input" name="gManufactureCode" type="text" value="$!{goods.gManufactureCode}" id="gManufactureCode" >
                                <label class="help-tips" id="gManufactureCode_info"></label>
                            </li>
                            <li>
                                <label>商品规格<font color="red">*</font>：</label>
                                <input class="input" name="gSpecifications" type="text" value="$!{goods.gSpecifications}" id="gSpecifications" onblur="isEmptyContent(this,'商品规格必填.',$('#gSpecifications_info'));">
                                <label class="help-tips" id="gSpecifications_info"></label>
                            </li>
                            <li>
                                <label>基本单位<font color="red">*</font>：</label>
                                <input class="input" name="gUnit" type="text" value="$!{goods.gUnit}" id="gUnit" onblur="isEmptyContent(this,'基本单位必填.',$('#gUnit_info'));">
                                <label class="help-tips" id="gUnit_info"></label>
                            </li>

                            <li>
                                <label>是否拆零销售<font color="red">*</font>：</label>
                                <input class="radio" type="radio" name="gCanSplit" id="radio_01" value="1" #if($goods.gCanSplit==1) checked="checked" #end onclick="gMiddlePackageSHCon(this.value);"/>
                            	<label class="text" for="radio_01">是</label>
                            	<input class="radio" type="radio" name="gCanSplit" id="radio_02"  value="0" #if($goods.gCanSplit==0) checked="checked" #end onclick="gMiddlePackageSHCon(this.value);"/>
                            	<label class="text" for="radio_02">否</label>
                            </li>
                            <li style="display:block;" id="gMiddlePackageLi1">
                                <label>中包装数量<font color="red">*</font>：</label>
                                <input class="input" name="gMiddlePackage" type="text" value="$!{goods.gMiddlePackage}" id="gMiddlePackage1"  min="0" max="99999" maxlength="11" onblur="isPInt(this,'中包装数量必填且为整数.',$('#gMiddlePackage_info1'));">
                                <label class="help-tips" id="gMiddlePackage_info1"></label>
                            </li>
                            <li style="display:none;" id="gMiddlePackageLi2">
                                <label>中包装数量：</label>
                                <input class="input" name="gMiddlePackage" type="text" value="$!{goods.gMiddlePackage}" id="gMiddlePackage2"  min="0" max="99999" maxlength="11" onblur="isPIntEmpty(this,'中包装数量为整数.',$('#gMiddlePackage_info2'));">
                                <label class="help-tips" id="gMiddlePackage_info2"></label>
                            </li>
                            <li>
                                <label>大包装数量：</label>
                                <input class="input" name="gBigPackage" type="text" value="$!{goods.gBigPackage}" id="gBigPackage" maxlength="11" onblur="isPIntEmpty(this,'大包装数量为整数.',$('#gBigPackage_info'));">
                                <label class="help-tips" id="gBigPackage_info"></label>
                            </li>

                            <li>
                                <label>商品状态<font color="red">*</font>：</label>
                                <input class="radio" type="radio" name="gStatus" id="radio_03" #if($goods.gStatus==0) checked="checked" #end value="0"/>
                            	<label class="text" for="radio_03">下架</label>
                            	<input class="radio" type="radio" name="gStatus" id="radio_04" #if($goods.gStatus==1) checked="checked" #end  value="1"/>
                            	<label class="text" for="radio_04">上架</label>
                            	<i class="i-tips-l">
                               	#if($goods.gdowntype==0) &nbsp; #else
		                           	质检不通过下架,
		                           	#if($goods.gdowntype==1) 无批准文号 #end
		                           	#if($goods.gdowntype==2) 批准文号过期、注销、错误 #end
		                           	#if($goods.gdowntype==3) 无规格或规格不明确 #end
		                           	#if($goods.gdowntype==4) 根据批文查询，名称错误 #end
		                           	#if($goods.gdowntype==5) 根据批文查询，规格错误 #end
		                           	#if($goods.gdowntype==6) 根据批文查询，厂家错误 #end
		                           	#if($goods.gdowntype==7) 其它 #end
                               	#end
                               	</i>
                           </li>
                            <li>
                                <label>商品条形码：</label>
                                <input class="input" name="gBarCode" type="text" value="$!{goods.gBarCode}" id="gBarCode">
                            </li>

                            <li>
                                <label>商品内码(供应商)：</label>
                                <input class="input" name="gInSn" type="text" value="$!{goods.gInSn}" id="gInSn" #if($isErp==1) disabled="disabled" #end>
                            </li>

                            #if($!utils.notEmpty($!taxrateList))
                                <li>
                                    <label>商品税率：</label>
                                    <select id="gTaxRate" name="gTaxRate" style="width: 60px;">
                                            #foreach($taxrate in $!taxrateList)
                                                #if($taxrate == $!{goods.gTaxRate})
                                                    <option selected value=$!taxrate>$!taxrate</option>
                                                #end

                                                #if($taxrate != $!{goods.gTaxRate})
                                                    <option value=$!taxrate>$!taxrate</option>
                                                #end
                                            #end
                                    </select>
                                </li>

                                <li>
                                    <label>商品税务码：</label>
                                    <input class="input" id="gTaxRateCode" name="gTaxRatecode" type="text" value="$!{goods.gTaxRateCode}"  maxlength="20" onkeyup="num(this);">
                                </li>
                            #end



                            #if($user.userType==2)
                            <li>
                                <label>商品销售控制：<font color="red">*</font>：</label>
                                <input class="radio" type="radio" name="gControl" id="radio_05" #if($goods.gControl==0) checked="checked" #end value="0"/>
                            	<label class="text" for="radio_05">普通药品</label>
                            	<input class="radio" type="radio" name="gControl" id="radio_06" #if($goods.gControl==1) checked="checked" #end value="1"/>
                            	<label class="text" for="radio_06">医院专供</label>
                           </li>
                           #end
                           #if($user.userType==3)
                           <li style="width: 900px;">
                               <label>商品库存<font color="red">*</font>：</label>
                               <input class="input_price" id="gcn" name="gcn" type="text" value="$!{goods.gcn}" onblur="isPIntEmpty(this,'库存数量为整数.',$('#label_gcn'));">
                               <input type="checkbox" name="oversale" id="oversale" #if($goods.oversale==1) checked #end value="1" onclick="showOversaleInfo()" style="float: left;>
                                <span style="vertical-align: middle;float: left;">允许超卖</span>
                               <span class="i-tips" id="label_oversale" style="margin-left: 12px;">#if($goods.oversale==1) 当商品库存售罄，用户还可以无限够买. #end</span>
                               <label class="help-tips" id="label_gcn" ></label>
                           </li>
                           <li>
                                <label>零售价<font color="red">*</font>：</label>
                                <input class="input_price" name="gPrice" type="text" value="$!{goods.gPrice}" id="gPrice" onblur="isPrice(this,'零售价必填且为整数或小数,小数位最多为四位.',$('#gPrice_info'));" maxlength="20">
                                <label class="help-tips" id="gPrice_info" ></label>
                            </li>
                           #end
                        </ul>
               		</form>
                </div>
                #if($user.userType==3)
                <h3 class="info_title">产品一级经销商</h3>
                <div class="info_sheet cc">
                    <div class="order_detail">
                        <div class="toogle-wrap" id="promotion_member">
                            <div class="tab-box">
                                <div class="tab-con">
                                    <ul id="customerList">
                                        <!--                                     <li> -->
                                        <!--                                         <div class="customer-item"><b></b>山东天元大药房</div> -->
                                        <!--                                     </li> -->
                                        <li><a href="javascript:;" class="add_pro" id="sel-a" onclick="addCusteomer()">+ 添加客户</a></li>
                                        #foreach($item in $goodsAgencyList)
                                        <li name='eIds' id='eIds_$item.agencyEId' value='$item.agencyEId' suId='$item.agencySuId'>
                                        	<div class='customer-item item-selected'><b onclick='delli($item.agencyEId)'></b>$item.agencyEName</div>
                                        </li>
                                        #end
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                #end
                <h3 class="info_title">商品分类信息</h3>
                <div class="info_sheet cc">
                	<form action="" method="post" id="">
                        <ul>
                            <li class="li_sort">
                                <label>商品分类<font color="red">*</font>：</label>
                                <div class="sltitem sltitem_top">
                                    <span class=""></span>
                                    <ul>
                                        <li data-value=""><a href="javascript:;"></a></li>
                                    </ul>
                                    <input type="hidden" name="sgcId1" id="sgcId1" value="$!{goods.sgcId1}">
                                </div>
                                <div class="sltitem sltitem_top">
                                    <span class=""></span>
                                    <ul>
                                        <li data-value=""><a href="javascript:;"></a></li>
                                    </ul>
                                   <input type="hidden" name="sgcId2" id="sgcId2" value="$!{goods.sgcId2}">
                                </div>
                                <label class="help-tips" id="sgcId1_info"></label>
                            </li>
                            <li>
                            #if($user.userType==2)
                                <label>商品类型：</label>
                                #if($goods.gType==3)
                                    <input class="radio" type="radio" name="gType" id="gType_04" value="3" disabled="true" checked="checked" />
                                #else

                                    <label class="text" for="gType_02" title="新品上架的商品统一会在旗舰商城“找新品”页面集中显示 去看看>>">
                                        <input class="check" type="checkbox" name="gType" id="gType_02" #if($!{goods.gType}==2)checked="checked"#end value="2" style="vertical-align: middle"/>
                                        新品上架
                                    </label>

                                    <label class="text" for="gType_03" title="诊所专区推荐的商品统一会在旗舰商城“诊所专区”频道中显示。去看看>>">
                                        <input class="check" type="checkbox" name="clinicSupplyFlag" id="gType_03" #if($!{goods.clinicSupplyFlag}==1)checked="checked"#end value="1" style="vertical-align: middle"/>
                                        诊所专区推荐
                                    </label>
                                #end
                            #end
                            </li>
                       #*    #if($user.userType==2)
                             <li>
                            	<label>备注：</label>
                                <input class="input_price" name="gRemark" type="text" value="$!{goods.gRemark}" id="gRemark"   maxlength="250"  >
                             </li>
                            #end*#
                            #if($user.userType==2)
                            <li class="li_sort">
                                <label>药品剂型分类：</label>
                                <div class="sltitem">
                                    <span class="" ></span>
                                    <ul>
                                    #if(!$null.isNull($!{goodsDosageFormsList}) && ($!goodsDosageFormsList.size()>0))
                                    #foreach($item in $goodsDosageFormsList)
                                        <li data-value="$!{item.gdfId}" ><a href="javascript:;">$item.gdfName</a></li>
                                    #end
                                    #end
                                    </ul>
                                    <input type="hidden" name="gdfId" id="gdfId">
                                </div>
                            </li>
                            #end
                            #if($user.userType==3)
                            <li class="area_box">
                             <label class="w132">配置销售区域<font color="red">*</font>：</label>
                               <div class="site_box">
                                #if(!$null.isNull($!{siteFactoryList}) && ($!siteFactoryList.size()!=0))
                                    <span><input type="checkbox" class="checkbox"  name="selectAllSite"  id="selectAllSite" onclick="selectAllSiteBox()"/>
                                     <label class="text" for="selectAllSite">全选</label></span>

                                    #foreach($item in $siteFactoryList)
                                        <span><input type="checkbox" class="checkbox" value="$!{item.siteId}"  name="siteId"  id="$item.siteId" #foreach($site in $siteGoodsList) #if($site.siteId==$item.siteId)checked="checked"#end#end />
                                        <label class="text" for="$!{item.siteId}">$item.siteName</label></span>
                                    #end
                                #end
                              </div>
                            </li>
                            #end
                        </ul>
               		</form>
                </div>
                #if($user.userType==2)
                <h3 class="info_title">价格和库存</h3>
                <div class="info_sheet cc info_box3 ">
                	<form action="" method="post" id="">
                        <ul>
                            <li>
                                <label>成本价</label>
                                <input class="input_price" name="gCost" type="text" value="$!{goods.gCost}" id="gCost" onblur="isPriceNotEmpty(this,'成本价为整数或小数,小数位最多为四位.',$('#gCost_info'));" maxlength="20">
                                <label class="help-tips" id="gCost_info" ></label>
                            </li>
                            <li>
                                <label>零售价<font color="red">*</font>：</label>
                                <input class="input_price" name="gPrice" type="text" value="$!{goods.gPrice}" id="gPrice" onblur="isPrice(this,'零售价必填且为整数或小数,小数位最多为四位.',$('#gPrice_info1'));" maxlength="20">
                                <label class="help-tips" id="gPrice_info1" ></label>
                            </li>
                            <li>
                                <label>商品基价<font color="red">*</font>：</label>
                                <input class="input_price" name="gBasePrice" type="text" value="$!{goods.gBasePrice}" id="gBasePrice" onblur="isPrice(this,'商品基价必填且为整数或小数,小数位最多为四位.',$('#gBasePrice_info'));" maxlength="20">
                                <label class="help-tips" id="gBasePrice_info" ></label>
                            </li>
                            <li style="width: 900px;">
                                <label>商品库存<font color="red">*</font>：</label>
                                <input class="input_price" id="gcn" name="gcn" type="text" value="$!{goods.gcn}" onblur="isPIntEmpty(this,'库存数量为整数.',$('#label_gcn'));">
                                <input type="checkbox" name="oversale" id="oversale" value="1" #if($goods.oversale==1) checked #end onclick="showOversaleInfo()" style="vertical-align: middle;float: left;">
                                <span>允许超卖</span>
                                <span class="i-tips" id="label_oversale" style="margin-left: 12px;">#if($goods.oversale==1) 当商品库存售罄，用户还可以无限够买. #end</span>
                                <label class="help-tips" id="label_gcn" ></label>
                            </li>

##                            #if(!$null.isNull($!{shopCustomerTypeList}) && ($!shopCustomerTypeList.size()>0))
##                            #foreach($item in $shopCustomerTypeList)
##                            <li>
##                                <label>$!{item.sctName}的库存数量<font color="red">*</font>：</label>
##                                <input class="input_price" id="$!{item.sctNumberField}" name="$!{item.sctNumberField}" type="text" value="$!goodsCustomNumber.get($!{item.sctNumberField})" onblur="isPIntEmpty(this,'库存数量为整数.',$('#label_$!{item.sctNumberField}'));">
##                                <label class="help-tips" id="label_$!{item.sctNumberField}" ></label>
##                            </li>
##                            #end
##                            #end
                        	<li class="li_one">
                            	<label>是否允许ERP同步覆盖：</label>
                                <input class="radio" type="radio" name="erpOverride" id="erpOverride_1" value="1"  #if($!{goods.erpOverride.equals(1)})checked="checked"#end/>
                            	<label class="text" for="erpOverride_1">允许</label>
                            	<input class="radio" type="radio" name="erpOverride" id="erpOverride_2" #if($!{goods.erpOverride.equals(0)})checked="checked"#end value="0" />
                            	<label class="text" for="erpOverride_2">不允许</label>
                            	<i class="i-tips">注：更新内容包括商品信息、商品上下架状态、商品库存、商品基价（不影响商品定价体系）</i>
                            </li>
                        </ul>
               		</form>
                </div>
                #end
                #if($user.userType==2)
                <h3 class="info_title">关键权限</h3>
                <div class="info_sheet cc">
                	<form action="" method="post" id="">
                        <ul>
                           <li>
                                <label>内外销<font color="red">*</font><i class="order-tips" data-tips="true" data-tips-title="
								商品内外销设置：<br>
								内销：经营区域为“区域内”的客户可购买此商品<br>
								外销：经营区域为“区域外”的客户可购买此商品<br>
								不限：所有有权限采购客户可购买此商品
								"></i>：</label>
                            	<input class="radio" type="radio" name="gArea" id="gArea_01" value="1" #if($!{goods.gArea}==1)checked="checked"#end />
                            	<label class="text" for="gArea_01">内销</label>
                            	<input class="radio" type="radio" name="gArea" id="gArea_02" value="2" #if($!{goods.gArea}==2)checked="checked"#end />
                            	<label class="text" for="gArea_02">外销</label>
                            	<input class="radio" type="radio" name="gArea" id="gArea_03"  value="0" #if($!{goods.gArea}==0)checked="checked"#end/>
                            	<label class="text" for="gArea_03">不限 </label>
                            </li>
                            <li>

                            </li>
                            <li>
                                <label>分单仓储地点<i class="order-tips" data-tips="true" data-tips-title="设置商品所属仓储地点，用于采购客户下单时，不同仓储地点的商品，系统自动拆单"></i>：</label>
                                #if(!$null.isNull($!{shopOrderCateList}) && ($!shopOrderCateList.size()>0))

   // 错误提示
   var showMsg = function (obj, msg, flag) {
       if (flag) {
           obj.attr("style", "color:green;font-weigh:700;").html("&#8730;");
       } else if (!msg) {
           obj.html('');
       }  else {
           msg.length > 0 && obj.attr("style", "color:red;font-weigh:100;").html("*" + msg);
       }
   };


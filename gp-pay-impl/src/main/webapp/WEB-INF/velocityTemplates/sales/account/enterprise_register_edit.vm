	   function checkAmount(obj){
		   obj.value = obj.value.replace(/[^\d]/g,'');
		   var value = obj.value; 
		   if(isEmpty(value)){
			   $("#isLimitAmount").hide();
		   }else if(parseInt(value) <= 0){
			   $("#suDistributionAmount").val("");
			   $("#isLimitAmount").hide();
		   }else{
			   $("#isLimitAmount").show();
		   }
	   }
	 	

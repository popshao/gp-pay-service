                    $('#form').submit();
                }
            });
	 	}
	 //验证
	 function checkForm(){
				$("#form").validate({
		            submitHandler: editAndAudit, //如果验证通过的回调函数
		            rules: {
		            	suLicenseNo: {
			                    required: true,
			                    minlength:15,
			                    maxlength: 50,
			               		remote: {
			                        type: "post",
			                        url: "/sales/account/enterprise_certified_checkELicenseNo.html",
			                        data: {
			                        	eLicenseNo: function() {
			                                return $("#suLicenseNo").val();
			                            }
			                        },
			                        dataType: "json",
			                        dataFilter: function(dataXML) {
			                        	var result = dataXML;
			                        	//初始的工商注册号
			                        	var initELicenseNo=$("#initELicenseNo").val();
			                        	//修改的工商注册号
			                        	var suLicenseNo=$("#suLicenseNo").val();
			                        	if(initELicenseNo==suLicenseNo){
			                        		return true;
			                        	}else{
			                        		if (result=="true")
				                                return false;
				                            else
				                                return true;
			                        	}
			                        }
			                    }
		            		},
			            	suName:{
			            		required: true,
			            		maxlength: 30
			            	},
			            	suTel:{
			            		isMobile:true
			            	},
			            	suMail:{
			            		required: false,
			            		email:true
			            	},
			            	suPhone:{
			            		isTelephone:true
			            	},
			            	suDistributionAmount:{
			            		min:10
			            	},
			            	suShopRemark:{
			            		maxlength: 1000
			            	}
		                },
		                messages: {
		                	suLicenseNo:{
			            		remote: "企业工商注册号已经存在！"
			            	}
			            },
	               errorPlacement: function(error, element) {
	                   error.appendTo(element.parent());
	               }
		        });
	 	}

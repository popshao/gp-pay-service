            $(obj).closest(".manage-item").find(".manage-con").each(function () {
                if(f){
                    $($(this).find(".input").get(0)).val($("#all_beginTime").val())
                    $($(this).find(".input").get(1)).val($("#all_endTime").val())
                }else{
                    $($(this).find(".input").get(0)).val("")

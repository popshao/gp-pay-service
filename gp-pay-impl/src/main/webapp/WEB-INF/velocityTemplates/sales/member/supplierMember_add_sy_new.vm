<script type="text/javascript">
    /*设置框架返回按钮*/
    top.utils.frameBack({
        render:true,
        handler:function (btn) {
            location.href = document.referrer;
        }
    })
    layui.config({
        base: '/js/layui/extend/'
    }).use(['addMember'], function () {
        var addMember = layui.addMember;
        addMember.run(function (layApp) {
            //执行本地代码
            $.Tipmsg.s="该项必填"
            var vf = $("#submitForm").Validform({
                tiptype:3,
                showAllError:true,
                postonce:true,
                ajaxPost:false,
                datatype:{
                    "xyed":function(vals,obj,form,reg){//信用额度
                        var status = "$!status";
                        if(status == 10){
                            var isPay2 = $("#pay2").prop("checked");
                            if(isPay2){
                                if(!/^[1-9][0-9]*$/.test(vals)){
                                    return false;
                                }
                            }
                        }
                        return true;
                    },
                    select:function (vals,obj,form,reg) {
                        var _ops= $(obj).find("option");
                        if(_ops.length > 0){
                            if(vals){
                                return true;
                            }else{
                                return false;
                            }
                        }else{
                            return true;
                        }
                    }
                },
                callback:function (form) {
                    addSave(layApp);
                    return false;
                }
            });
            $("#btnSubmit").on("click",function () {
                vf.submitForm();
            })
            setTimeout(function () {
                $("select").on("change",function () {
                    vf.check(false,$(this));
                })
            },500);
        });
    })

    function addSave(layApp) {

        //判断企业类型进行remove元素
        //企业类型非医院和诊所(必填资质)和企业类型非医院和诊所(非必填资质)
        //验证必填的图片是否上传
        var saImgPath1=$("#saImgPath1").val();
        var saImgPath2=$("#saImgPath2").val();
        var saImgPath3=$("#saImgPath3").val();

        var epStartTime1=$("#epStartTime1").val();
        var epStartTime2=$("#epStartTime2").val();
        var epStartTime3=$("#epStartTime3").val();

        var epEndTime1=$("#epEndTime1").val();
        var epEndTime2=$("#epEndTime2").val();
        var epEndTime3=$("#epEndTime3").val();

        if(saImgPath1!=""){
//            if(epStartTime1==""||epEndTime1==""){
//                layApp.alert({
//                    head:"提示信息",
//                    text:"必须填写企业营业执照有效期"
//                })
//                return;
//            }
        }

        if(saImgPath2!=""){
//            if(epStartTime2==""||epEndTime2==""){
//                layApp.alert({
//                    head:"提示信息",
//                    text:"必须填写药品经营许可证有效期"
//                })
//                return;
//            }
        }

        if(saImgPath3!=""){
//            if(epStartTime3==""||epEndTime3==""){
//                layApp.alert({
//                    head:"提示信息",
//                    text:"必须填写GSP证书有效期"
//                })
//                return;
//            }
        }
        //企业类型是医院和诊所(必填资质)和企业类型非医院和诊所(非必填资质)
        if(eType==6 || eType==7){
            $("#box2").remove();
            $("#box3").remove();
            $("#box6").remove();
            $("#box7").remove();
        }
        //序列化表单
        var supplierMemberWrapper = $("#submitForm").serializeJson();
        supplierMemberWrapper.smStatus = $("#smStatus").val();
        //资质审核状态
        var userEname = "$!userEname";
        if(eType==6 || eType==7){
            var saImgPath1=$("#saImgPath1").val();
            var saImgPath4=$("#saImgPath4").val();
            var saImgPath5=$("#saImgPath5").val();
            var saImgPath8=$("#saImgPath8").val();
            var saImgPath9=$("#saImgPath9").val();
            var saImgPath13=$("#saImgPath13").val();
            if(saImgPath1 == "" && saImgPath4 == "" && saImgPath5 == "" && saImgPath8 == "" && saImgPath9 == "" && saImgPath13 == ""){
                supplierMemberWrapper.picStatus = 2;
            }else{
                supplierMemberWrapper.picStatus = 3;
            }
        }else{
            var saImgPath1=$("#saImgPath1").val();
            var saImgPath2=$("#saImgPath2").val();
            var saImgPath3=$("#saImgPath3").val();
            var saImgPath4=$("#saImgPath4").val();
            var saImgPath5=$("#saImgPath5").val();
            var saImgPath6=$("#saImgPath6").val();
            var saImgPath7=$("#saImgPath7").val();
            var saImgPath8=$("#saImgPath8").val();
            var saImgPath9=$("#saImgPath9").val();
            if(saImgPath1 == "" && saImgPath2 == "" && saImgPath3 == "" && saImgPath4 == "" && saImgPath5 == "" && saImgPath6 == "" && saImgPath7 == "" && saImgPath8 == "" && saImgPath9 == ""){
                supplierMemberWrapper.picStatus = 2;
            }else{
                supplierMemberWrapper.picStatus = 3;
            }
        }
//    		supplierMemberWrapper.picStatus = 3;
        supplierMemberWrapper.uploadEnterprise = userEname;
        //权限
        var flag = false;
        var purs = "";
        var purviews = new Array;
        $("input[name^='selck']").each(function(i, o) {
            if (flag) {
                return;
            }
            if (o.checked) {
                var beginTime = $('#' + o.id + "_beginTime").val();
                if (!beginTime) {
                    layApp.alert({
                        head:"提示信息",
                        text:"请选择【" + o.title + "】经营期限开始时间"
                    })
                    flag = true;
                    return;
                }
                var endTime = $('#' + o.id + "_endTime").val();
                if (!endTime) {
                    layApp.alert({
                        head:"提示信息",
                        text:"请选择【" + o.title + "】经营期限结束时间"
                    })
                    flag = true;
                    return;
                }
                flag = false;
                if (purs != "") {
                    purs += ",";
                }
                purs += o.id;
                var purobj = {
                    spId : o.id,
                    smpStartTime : beginTime,
                    smpEndTime : endTime
                };
                purviews.push(purobj);
            }
        });
        if (flag) {
            return;
        }

        if (purs == "") {
            layApp.alert({
                head:"提示信息",
                text:"请选择采购权限"
            })
            return;
        }
        //参数
        var eType = $("#eType").val();
        var roleCode = '';
        if(eType == 1){
            roleCode = 'sys004';
        }else if(eType == 2){
            roleCode = 'sys001';
        }else if(eType == 3){
            roleCode = 'sys002';
        }else if(eType == 4){
            roleCode = 'sys003';
        }else if(eType == 5){
            roleCode = 'sys008';
        }else if(eType == 6){
            roleCode = 'sys005';
        }else if(eType == 7){
            roleCode = 'sys006';
        }
        var purviewsParam = $.toJSON(purviews);
        layer.load(2,{shade:[.8]});
        $.ajax({
            type : 'post',
            url : '/sales/member/supplierMember_addSave.html',
            data : {
                "supplierMemberWrapper" : $.toJSON(supplierMemberWrapper),
                "purviews" : purviewsParam,
                "roleCode" : roleCode
            },
            dataType : 'json',
            success : function(data) {
                if (data) {
                    layApp.alert({
                        head:"提示信息",
                        text:"保存成功",
                        ok:function () {
                            top.utils.frameBackDel();
                            location = "/sales/member/member_info.html";
                        }
                    })
                } else {
                    layer.closeAll("loading");
                    layApp.alert({
                        head:"提示信息",
                        text:"保存失败"
                    })
                }
            }
        });
    }
